{
  "inputs": [
    {
      "id": "arg0",
      "type": "promptString",
      "description": "Optional: connect to an already-running Chrome (remote debugging / port-forward). Example: http://127.0.0.1:9222",
      "default": ""
    },
    {
      "id": "arg1",
      "type": "promptString",
      "description": "Run Chrome headless (true/false). Default: false",
      "default": "false"
    },
    {
      "id": "arg2",
      "type": "promptString",
      "description": "Use a temporary user-data-dir (true/false). Default: false",
      "default": "false"
    },
    {
      "id": "arg3",
      "type": "promptString",
      "description": "Chrome channel: stable | canary | beta | dev (default: stable).",
      "default": "stable"
    }
  ],
  "servers": {
    "uno": {
      "type": "sse",
      "url": "https://mcp.platform.uno/v1",
      "headers": {}
    },
    "GitHub": {
      "type": "sse",
      "url": "https://api.githubcopilot.com/mcp/",
      "internalMetadata": {
        "description": "Official GitHub MCP Server that connects AI tools directly to GitHub's platform. Enables AI agents to read repositories, manage issues and PRs, analyze code, and automate workflows through natural language interactions.",
        "version": "0.13.0",
        "repository": "https://github.com/github/github-mcp-server",
        "id": "github/github-mcp-server",
        "publishedDate": "22.08.2025 00:00:00",
        "registry": "https://api.mcp.github.com/v0.1/servers/"
      }
    },
    "Chrome DevTools": {
      "type": "stdio",
      "command": "npx",
      "args": [
        "-y",
        "chrome-devtools-mcp@latest",
        "--browserUrl",
        "${input:arg0}",
        "--headless",
        "${input:arg1}",
        "--isolated",
        "${input:arg2}",
        "--channel",
        "${input:arg3}"
      ],
      "internalMetadata": {
        "description": "Lets your coding agent (such as Gemini, Claude, Cursor or Copilot) control and inspect a live Chrome browser.",
        "version": "0.0.1-seed",
        "repository": "https://github.com/ChromeDevTools/chrome-devtools-mcp",
        "id": "chromedevtools/chrome-devtools-mcp",
        "publishedDate": "09.09.2025 10:55:22",
        "registry": "https://api.mcp.github.com/v0.1/servers/"
      }
    }
  }
}