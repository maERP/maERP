{
  "AppConfig": {
    "Environment": "Production"
  },
  "ApiClientOptions": {
    "BaseUrl": "https://api.maerp.com",
    "ApiVersion": "1.0",
    "TimeoutSeconds": 30,
    "UseNativeHandler": true,
    "MaxRetryAttempts": 3,
    "EnableDetailedLogging": false
  },
  "WebAuthentication": {
    "LoginStartUri": "",
    "LogoutStartUri": ""
  },
  "LocalizationConfiguration": {
    "Cultures": [
      "es",
      "fr",
      "pt-BR",
      "en"
    ]
  }
}
